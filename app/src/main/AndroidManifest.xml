    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ui.request_flow.Activities.SelectShopperActivity"
            android:label="@string/title_activity_select_shopper" >
          <intent-filter>
            <action android:name="android.intent.action.MAIN"/>
            <category android:name="android.intent.category.LAUNCHER"/>
          </intent-filter>
        </activity>
        <activity
            android:name=".ui.request_flow.Activities.RequestStateActivity"
            android:label="@string/title_activity_request_state" >
        </activity>
    </application>

</manifest>
